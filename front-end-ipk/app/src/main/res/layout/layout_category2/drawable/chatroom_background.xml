    <!-- Define the background properties like color etc -->
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">

            <gradient
                android:startColor="#000000"
                android:centerColor="#181818"
                android:centerY="0.5"
                android:endColor="#383838"
                android:angle="225"
                />
        </shape>
    </item>
